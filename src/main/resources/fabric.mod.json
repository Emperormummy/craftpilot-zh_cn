{
	"schemaVersion": 1,
	"id": "craftpilot",
	"version": "${version}",
	"name": "Craftpilot",
	"description": "AI based building assistant.",
	"authors": [
		"mmmfrieddough"
	],
	"contact": {
		"homepage": "https://github.com/mmmfrieddough/craftpilot",
		"sources": "https://github.com/mmmfrieddough/craftpilot"
	},
	"license": "MIT",
	"icon": "assets/craftpilot/icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			"mmmfrieddough.craftpilot.CraftPilot"
		],
		"modmenu": [
			"mmmfrieddough.craftpilot.config.ModMenuIntegration"
		]
	},
	"mixins": [
		"craftpilot.mixins.json"
	],
	"depends": {
		"minecraft": ">=${minecraft_version}",
		"fabricloader": ">=${loader_version}",
		"fabric-api": ">=fabric_version",
		"cloth-config": ">=${cloth_config_version}"
	},
	"suggests": {
		"modmenu": ">=${modmenu_version}"
	},
	"custom": {
		"mc-publish": {
			"loaders": [
				"fabric"
			],
			"curseforge": 1174507
		}
	}
}